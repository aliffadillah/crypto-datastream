{
  "version": 2,
  "builds": [
    {
      "src": "nuxt.config.ts",
      "use": "@vercel/static-build",
      "config": {
        "distDir": ".output/public"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/.*",
      "dest": "/api/index"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
